                <operation id="oe80d08a64bc24b129242d98cc3f4e27f470" cid="OP237" name="OnAttackButton" creation-date="07/02/2021 13:33:05" type="" type-id="" visibility="public">
                  <source-codes>
                    <source-code language="CSharp">
        if(state == BattleState.PLAYERTURN) {
