                <operation id="oca0fee5c1620441b9acb554b1e3e360f415" cid="OP484" name="Start" creation-date="07/02/2021 13:45:28" type="" type-id="" visibility="undefined">
                  <documentation>
                    <text>
                      <default>
                        <style font-name="Verdana" font-size="12">
                          <text>
                            <solid-brush color="#FF000000" />
                          </text>
                        </style>
                      </default>
                      <styles />
                      <content>
                        <paragraph>
                          <style />
                          <p>
                            <t>Copy current position n stuff as long as there are Enemies 
Remove Enemies in the list from the world
Remove Items in the list from the world
</t>
                          </p>
                        </paragraph>
                      </content>
                    </text>
                  </documentation>
                  <source-codes>
                    <source-code language="CSharp">
        
        
        if (b_firstLoad)
        {
            if (GameObject.Find("Character"))
                m_playerposition = GameObject.Find("Character").transform.position;
            b_firstLoad = false;
            DontDestroyOnLoad(gameObject);
           
        }
        else
        {
            DontDestroyOnLoad(gameObject);

            for (int j = 0; j &lt; Object.FindObjectsOfType&lt;WorldComponents&gt;().Length; j++)
                if (Object.FindObjectsOfType&lt;WorldComponents&gt;()[j] != this)
                    if (Object.FindObjectsOfType&lt;WorldComponents&gt;()[j].name == gameObject.name)
                        Destroy(gameObject);

                                                                         
            if (GameObject.Find("Enemies"))
                GameObject.Find(s_playerobjectname).transform.position = m_playerposition;

                                                       
            if (!b_enemyDefeated)
            { 
                if (m_enemies.Count &gt; 0)
                    m_enemies.RemoveAt(m_enemies.Count - 1);
            }
            else
                b_enemyDefeated = false;

            for (int j = 0; j &lt; m_enemies.Count; j++)
                Destroy(GameObject.Find(m_enemies[j].ToString()));
            
                                                     
            for (int j = 0; j &lt; m_items.Count; j++)
                Destroy(GameObject.Find(m_items[j].ToString()));
        }
      
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o70d1b5201b424b42b8453018816695d4415" cid="OP485" name="reset" creation-date="07/02/2021 13:45:28" type="" type-id="" visibility="public" static="true">
                  <source-codes>
                    <source-code language="CSharp">
        m_enemies = new ArrayList();
        m_itemsAndArtifacts = new ArrayList();
        b_firstLoad = true;
        b_ringquest = false;
        s_playerobjectname = "Character";
        mage = new Mage();
        priest = new Priest();
        warrior = new Warrior();
        thief = new Thief();
        items = new List&lt;ItemClass&gt;();
        m_items = new ArrayList();
        CharacterController.b_Stop = false;
    </source-code>
                  </source-codes>
                </operation>
