                <operation id="oc00b4f40dfbc412db0cd2076201c6c7082" cid="OP389" name="PlayerTurn" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="undefined">
                  <source-codes>
                    <source-code language="CSharp">
        if (state == BattleState.WON || state == BattleState.LOST) {
            endBattle();
        } else if (playTurns.Count &gt; 0 &amp;&amp; typeof(PlayerClass).IsInstanceOfType(playTurns.Peek())) {
            state = BattleState.PLAYERTURN;
            if (playTurns.Peek().b_isAlive) {
                textchanger.setLog(playTurns.Peek().s_name + " choose an action: ");
            } else {
                playTurns.Dequeue();
                PlayerTurn();
            }
        } else if (playTurns.Count &gt; 0 &amp;&amp; typeof(MonsterClass).IsInstanceOfType(playTurns.Peek())) {
            state = BattleState.ENEMYTURN;
            EnemyTurn();

        } else {

            addCharstoList();
            playTurns = findFastesCharacters(characters);
            PlayerTurn();
        }
    </source-code>
                  </source-codes>
                </operation>
