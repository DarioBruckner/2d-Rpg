                    <source-code language="CSharp">
        if(b_loaded)
        {
            WorldComponents.m_playerposition = other.transform.position;
            WorldComponents.m_enemies.Add(this.transform.parent.name);
            b_loaded = false;
        }
        
                                        
                                                                                                                                                                                                      
                                                                                                                                          
