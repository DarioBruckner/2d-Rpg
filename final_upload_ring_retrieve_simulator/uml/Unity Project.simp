                <operation id="oaf6dacc5b0484f64b35d59070788b6ce82" cid="OP392" name="EnemyTurn" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="undefined">
                  <source-codes>
                    <source-code language="CSharp">
        int rng = 0;
        while (true) {
            rng = Random.Range(0, targets.Count);
            if (targets[rng].b_isAlive) {
                break;
            }

        }

        if(state == BattleState.ENEMYTURN) {
            
            targets[rng].takePhysDamage(playTurns.Peek().n_strength);
            textchanger.setHealthChar(rng + 1, targets[rng].n_HP);
            StartCoroutine(EnemyAttack(targets[rng]));
            

        }

       
        
