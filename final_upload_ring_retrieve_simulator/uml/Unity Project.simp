                <attribute id="a2a4d3e10de6f4bcc9e215e5d00856c5a90" cid="PROP363" name="m_BackgroundSpriteRenderer" creation-date="07/02/2021 13:44:32" property-ref="026eb85f025248d6bc76602f810378de" />
                <attribute id="a8af98a63022d4195a19eb73ea2fd4a7b90" cid="PROP364" name="m_TextMesh" creation-date="07/02/2021 13:44:32" property-ref="4d3dc0897b1d45158585507e51208489" />
