                <operation id="o69ff257a6e2b41e38471efd9daba7bde90" cid="OP421" name="Update" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="undefined">
                  <documentation>
                    <text>
                      <default>
                        <style font-name="Verdana" font-size="12">
                          <text>
                            <solid-brush color="#FF000000" />
                          </text>
                        </style>
                      </default>
                      <styles />
                      <content>
                        <paragraph>
                          <style />
                          <p>
                            <t> Update is called once per frame
</t>
                          </p>
                        </paragraph>
                      </content>
                    </text>
                  </documentation>
                  <source-codes>
                    <source-code language="CSharp">
        if (Input.GetKeyDown(KeyCode.Escape))
        {
            if (b_isPaused)
            {
                Resume();
            } else
            {
                Pause();
            }
        }
    </source-code>
                  </source-codes>
