                <operation id="ofc182e296f12442893d4a92f907626cd474" cid="OP248" name="runFromBattle" creation-date="07/02/2021 13:33:05" type="" type-id="" visibility="undefined">
                  <source-codes>
                    <source-code language="CSharp">
        int rng = Random.Range(0, 100);
        StartCoroutine(runAttempt(rng));
       
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o0e85e2105ac9424c965f9cd63f77c5e8474" cid="OP249" name="runAttempt" creation-date="07/02/2021 13:33:05" type="IEnumerator" type-id="IEnumerator" visibility="undefined">
                  <parameters>
                    <parameter id="p5b4f020884d848e194d406c0f45b2e11" name="rng" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">

        if (rng &lt; 50) {
            textchanger.setLog("The way was blocked by the enemy");
            playTurns.Dequeue();
            
        } else {
            textchanger.setLog("You Successfuly escaped");
            state = BattleState.RUN; 
        }


        
        yield return new WaitForSeconds(2f);
        if (state == BattleState.RUN) {
            StartCoroutine(changeLevel());
        } else {
            PlayerTurn();
        }
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o190d1f97adfa43ada47d99f58f788fb2474" cid="OP250" name="endBattle" creation-date="07/02/2021 13:33:05" type="" type-id="" visibility="undefined">
                  <source-codes>
                    <source-code language="CSharp">
        if(state == BattleState.WON) {
            textchanger.setLog("You won, well done");
            StartCoroutine(changeLevel());
        }else if (state == BattleState.LOST) {
            textchanger.setLog("Oh no you lost, try again!");
            StartCoroutine(changeLevel());
        }
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o3b8cd885a0914cb8986780f6ad1b3c91474" cid="OP251" name="changeLevel" creation-date="07/02/2021 13:33:05" type="IEnumerator" type-id="IEnumerator" visibility="undefined">
                  <source-codes>
                    <source-code language="CSharp">

        yield return new WaitForSeconds(2f);

        loader.LoadWorld();
    </source-code>
