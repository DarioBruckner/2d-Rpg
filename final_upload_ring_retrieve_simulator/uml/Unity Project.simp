                <operation id="o2de9644f845b4ba6840cd113305f90a5474" cid="OP245" name="PlayerHeal" creation-date="07/02/2021 13:33:05" type="IEnumerator" type-id="IEnumerator" visibility="undefined">
                  <parameters>
                    <parameter id="pff04944544c246bd849d57da3b00eaa2" name="ability" type="AbilityClass" type-id="AbilityClass" />
                    <parameter id="p4fad83ab94084d8bb129779be96b27d2" name="targetPlayer" type="PlayerClass" type-id="PlayerClass" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        CharacterClass user = playTurns.Peek();
        string abilityname = ability.s_name;
        string username = user.s_name;
        string targetname = targetPlayer.s_name;

       
        if(ability.allyAction(ref user, ref targetPlayer)) {
            textchanger.setManaByName(username, user.n_MP);
            textchanger.setHealthCharByName(targetname, targetPlayer.n_HP);
            textchanger.setLog(username + " healed " + targetname);
        } else {
            textchanger.setLog("Not enough mana to cast " + abilityname);
        }
        deactivateAllButtons();
        yield return new WaitForSeconds(2f);
        
        activateAllButtons();

        createDefaultButtons();
        playTurns.Dequeue();
        PlayerTurn();

    </source-code>
