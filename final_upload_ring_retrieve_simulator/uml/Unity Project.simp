                <operation id="o73439e3c780d4fbcbc8812159c7b21ac90" cid="OP467" name="Create" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public" static="true">
                  <parameters>
                    <parameter id="p5753712d5fe94a4399e1aa2266f301da" name="prefab" type="Transform" type-id="Transform" />
                    <parameter id="pc3defdb28cf147b5b88ab453392db766" name="parent" type="Transform" type-id="Transform" />
                    <parameter id="pa63357fb97ad4dcbb52a2cd939779cb4" name="localPosition" type="Vector3" type-id="Vector3" />
                    <parameter id="pd665ee086d8e4112856d71bcb71165f6" name="text" type="string" type-id="string" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        Transform textBubble = Instantiate(prefab, parent);
        if (parent.transform.localScale.x &lt; 0)
        {
            textBubble.localScale = new Vector3(textBubble.localScale.x * -1, textBubble.localScale.y, textBubble.localScale.z);
        }
        textBubble.localPosition = localPosition;
        textBubble.GetComponent&lt;TextBubble&gt;().Setup(text);
        Destroy(textBubble.gameObject, 5f);
    </source-code>
                  </source-codes>
                </operation>
                <operation id="obb9d0c8b6cd84bc6843f317d48ec8dad98" cid="OP468" name="Awake" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="private">
                  <source-codes>
                    <source-code language="CSharp">
        m_BackgroundSpriteRenderer = transform.Find("TextBubble").GetComponent&lt;SpriteRenderer&gt;();
        m_TextMesh = transform.Find("Text").GetComponent&lt;TextMeshPro&gt;();
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o41abaf3a502349598c6ed02603932b7c98" cid="OP469" name="Setup" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="private">
                  <parameters>
                    <parameter id="p264ce44dd9c0475fb4052d141d7b3547" name="text" type="string" type-id="string" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        m_TextMesh.SetText(text);
        m_TextMesh.ForceMeshUpdate();
        Vector2 textSize = m_TextMesh.GetRenderedValues(false);
        Vector2 padding = new Vector2(2f, 1f);
        Vector3 offset = new Vector3(-1f, -0.25f, 0);

        m_BackgroundSpriteRenderer.size = textSize + padding;
        m_BackgroundSpriteRenderer.transform.localPosition = new Vector3(m_BackgroundSpriteRenderer.size.x / 2, 0, 
                                                                m_BackgroundSpriteRenderer.transform.localPosition.z) + offset;
    </source-code>
                  </source-codes>
                </operation>
              </operations>
