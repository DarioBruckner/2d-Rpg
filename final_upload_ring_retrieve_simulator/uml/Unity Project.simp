                <operation id="o53f3c2be844f4f799e384c0dfa37cb61474" cid="OP244" name="SpecialAttack" creation-date="07/02/2021 13:33:05" type="IEnumerator" type-id="IEnumerator" visibility="undefined">
                  <parameters>
                    <parameter id="p345e7b007075499599e65837c4ea97b8" name="abilityname" type="string" type-id="string" />
                    <parameter id="p4520991d23f54a5e95c71c7f4b407856" name="username" type="string" type-id="string" />
                    <parameter id="p703944bbbe23415bb02693e859210fff" name="targetname" type="string" type-id="string" />
                    <parameter id="pb8841ba819c3406ba9d4a32364ea55a6" name="mana" type="bool" type-id="bool" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        if (mana) {
            textchanger.setLog(username + " used " + abilityname + " on " + targetname);
            playTurns.Dequeue();
        } else {
            textchanger.setLog("You dont have enough mana for " + abilityname);
        }
        deactivateAllButtons();

        yield return new WaitForSeconds(2f);

        activateAllButtons();

        if (!Enemy.b_isAlive) {
            state = BattleState.WON;
            endBattle();
        } else {
            PlayerTurn();
        }
    </source-code>
