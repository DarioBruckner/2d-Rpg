                  <parameters>
                    <parameter id="pfa8f799bddca41b38b6bb93552e90332" name="prefab" type="Transform" type-id="Transform" />
                    <parameter id="p12e0abab0de24e64a7c1c09ea32b402e" name="parent" type="Transform" type-id="Transform" />
                    <parameter id="p2571b2c6b57546ab94b46c5445843f0e" name="localPosition" type="Vector3" type-id="Vector3" />
                    <parameter id="pc59cf9504a6c4bb3a7823acde99ba239" name="text" type="string" type-id="string" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        Transform textBubble = Instantiate(prefab, parent);
        if (parent.transform.localScale.x &lt; 0)
        {
            textBubble.localScale = new Vector3(textBubble.localScale.x * -1, textBubble.localScale.y, textBubble.localScale.z);
        }
        textBubble.localPosition = localPosition;
        textBubble.GetComponent&lt;TextBubble&gt;().Setup(text);
        Destroy(textBubble.gameObject, 5f);
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o55fa71ff767b48b68c0e5a9379d67f11840" cid="OP305" name="Awake" creation-date="07/02/2021 13:42:32" type="" type-id="" visibility="private">
                  <source-codes>
                    <source-code language="CSharp">
        m_BackgroundSpriteRenderer = transform.Find("TextBubble").GetComponent&lt;SpriteRenderer&gt;();
        m_TextMesh = transform.Find("Text").GetComponent&lt;TextMeshPro&gt;();
    </source-code>
                  </source-codes>
