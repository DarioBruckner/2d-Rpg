                    <parameter id="p6726060ccff94440b86be9b754eb2c95" name="enemyHealth" type="int" type-id="int" />
                    <parameter id="p7e697fec05cb446e8b16dc8bb46d59d5" name="manachar1" type="int" type-id="int" />
