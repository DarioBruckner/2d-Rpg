                  <parameters>
                    <parameter id="pca7793fb3c564aa78ff1fbd5e9730ae7" name="healthchar1" type="int" type-id="int" />
                    <parameter id="pbbd2c982cabc4eccb76224dced6977d9" name="healthchar2" type="int" type-id="int" />
