                </operation>
                <operation id="oc9d4a66c855c4ed8a17514ea518cf0f374" cid="OP361" name="regenerateMP" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public">
                  <parameters>
                    <parameter id="p10eb5e03e61d47b98b7f7192ae1318b2" name="amount" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        this.n_MP += amount;
        if (this.n_MP &gt; this.n_maxMP)
            this.n_MP = this.n_maxMP;
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o12015ee92c034cb8a9fbb3621cbafcd974" cid="OP362" name="revive" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public">
                  <parameters>
                    <parameter id="pce0f42672b9e4cb68147445e51edf057" name="restore" type="double" type-id="double" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        b_isAlive = true;
        int heal = (int)Math.Ceiling((double)(this.n_maxHP * restore));
        this.getHealed(heal);
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o649613f972ed419cbf424d0b927ba4f174" cid="OP363" name="takePhysDamage" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public">
                  <parameters>
                    <parameter id="p26479536dfa440b0b8b4297fc8e22a94" name="atkStrength" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        this.n_HP -= atkStrength;
        if (this.n_HP &lt;= 0)
        {
            this.b_isAlive = false;
            this.n_HP = 0;
        }
    </source-code>
                  </source-codes>
                </operation>
                <operation id="ob730b3c1a66e4a4c97ebb29420a1c37474" cid="OP364" name="takeMagicDamage" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public">
                  <parameters>
                    <parameter id="p0f1049d2f3b24bbe8630004c2fa7c469" name="atkMM" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        this.n_HP -= atkMM;
        if (this.n_HP &lt;= 0)
        {
            this.b_isAlive = false;
            this.n_HP = 0;
        }
    </source-code>
                  </source-codes>
                </operation>
                <operation id="oa98a2e165f0a4933936eb7e1f25dbefd74" cid="OP365" name="getHealed" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public">
                  <parameters>
                    <parameter id="pab6de75870fd413da02ff1cb2d4a5fa9" name="heal" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        if ((heal + this.n_HP) &gt; this.n_maxHP)
            this.n_HP = this.n_maxHP;

        else
            this.n_HP += heal;
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o55e5f753788a4496bfdc7b8bad80a24274" cid="OP366" name="attack" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="public" virtual="true">
                  <parameters>
                    <parameter id="pcbabad926ef34caeb724c7ecbf263e23" name="target" type="CharacterClass" type-id="CharacterClass" direction="Out" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        double rawDamage = (this.n_strength / target.n_vitality) + 1;
        System.Random rng = new System.Random();
        double crit = rng.Next(100) * ((this.n_agility / 100) + 1);
        if (crit &gt; 90)
            rawDamage *= 2;

        int damage = (int)Math.Ceiling(rawDamage);
        target.takePhysDamage(damage);
    </source-code>
                  </source-codes>
                </operation>
                <operation id="ob9bdf32f0fb645979380d3fc1621a73174" cid="OP367" name="gainExp" creation-date="07/02/2021 13:44:32" type="bool" type-id="bool" visibility="public">
                  <parameters>
                    <parameter id="pdd8502aecad74874af4c526a431acb66" name="gain" type="int" type-id="int" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        bool leveled = false;
        if (gain &gt; 0)
        {
            this.n_exp += gain;
        }
        while (this.n_exp &gt;= this.n_expReq)
        {
            this.levelUp();
            leveled = true;
        }
        return leveled;
    </source-code>
                  </source-codes>
                </operation>
              </operations>
