                <operation id="od1332ce2d76f40a3afac87777ff28864415" cid="OP472" name="LoadWorld" creation-date="07/02/2021 13:45:28" type="" type-id="" visibility="public">
                  <source-codes>
                    <source-code language="CSharp">
        StartCoroutine(Loader("level_0"));
        CharacterController.b_Stop = false;
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o168932a90d094d6a93f83fe9c6a74061415" cid="OP473" name="LoadMainMenu" creation-date="07/02/2021 13:45:28" type="" type-id="" visibility="public">
                  <source-codes>
                    <source-code language="CSharp">
        StartCoroutine(Loader("main menu"));
        Time.timeScale = 1f;
    </source-code>
