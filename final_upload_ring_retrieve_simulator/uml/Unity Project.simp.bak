                <operation id="o629a6e6c79e642d99cf15678d776ab49470" cid="OP232" name="findFastesCharacters" creation-date="07/02/2021 13:33:05" type="Queue&lt;CharacterClass&gt;" type-id="Queue&lt;CharacterClass&gt;" visibility="public">
                  <parameters>
                    <parameter id="pdfe9391c77564c19aa491fff060869af" name="charchters" type="List&lt;CharacterClass&gt;" type-id="List&lt;CharacterClass&gt;" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        Queue&lt;CharacterClass&gt; characterQueue = new Queue&lt;CharacterClass&gt;();
        int size = charchters.Count;
        for(int i = 0; i&lt; size; i++) {
            int speed = 0;
            CharacterClass tempobj = null;
            foreach (CharacterClass cha in charchters) {
                if(cha.n_agility &gt; speed) {
                    speed = cha.n_agility;
                    tempobj = cha;
                }
            }
            characterQueue.Enqueue(tempobj);
            charchters.Remove(tempobj);
            speed = 0;
        }

        return characterQueue;
    </source-code>
                  </source-codes>
                </operation>
