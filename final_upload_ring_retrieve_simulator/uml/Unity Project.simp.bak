                  <parameters>
                    <parameter id="pfa8f799bddca41b38b6bb93552e90332" name="prefab" type="Transform" type-id="Transform" />
                    <parameter id="p12e0abab0de24e64a7c1c09ea32b402e" name="parent" type="Transform" type-id="Transform" />
                    <parameter id="p2571b2c6b57546ab94b46c5445843f0e" name="localPosition" type="Vector3" type-id="Vector3" />
