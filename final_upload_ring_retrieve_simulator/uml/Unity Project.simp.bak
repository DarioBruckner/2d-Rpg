                <operation id="oa82dd727658540229a8ac43a4db96052542" cid="OP722" name="Start" creation-date="07/05/2021 14:41:02" type="" type-id="" visibility="undefined">
                  <documentation>
                    <text>
                      <default>
                        <style font-name="Verdana" font-size="12">
                          <text>
                            <solid-brush color="#FF000000" />
                          </text>
