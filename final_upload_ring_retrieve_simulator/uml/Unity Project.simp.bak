                  <source-codes>
                    <source-code language="CSharp">
        if (user.drainMP(10))
        {
            target.revive(this.n_lvl * 0.1);
            this.n_uses++;
            if (this.n_uses % 5 == 0)
            {
                this.levelUp();
            }
