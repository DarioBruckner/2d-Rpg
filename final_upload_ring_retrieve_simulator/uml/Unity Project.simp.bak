                <operation id="obb1627fb3aca45b0a60e3c337887eed690" cid="OP458" name="Start" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="undefined">
                  <documentation>
                    <text>
                      <default>
                        <style font-name="Verdana" font-size="12">
                          <text>
                            <solid-brush color="#FF000000" />
                          </text>
