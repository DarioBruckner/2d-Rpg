                      <content>
                        <paragraph>
                          <style />
                          <p>
                            <t>generates random amount of Attacks between 1 and 6
Attack was successful
Error: Not enough Magic Power
 Start is called before the first frame update
</t>
                          </p>
                        </paragraph>
                      </content>
                    </text>
                  </documentation>
                  <source-codes>
                    <source-code language="CSharp">
        if (user.drainMP(3))
        {
                                                                
            System.Random rng = new System.Random();
            int attacks = rng.Next(1, 6);
            for (int i = 0; i &lt; attacks; i++)
            {
                double rawDamage = ((user.n_strength * 0.75) / target.n_vitality) + 1;
                double crit = rng.Next(100) * ((user.n_agility / 100) + 1);
                if (crit &gt; 80)
                    rawDamage *= 2;

                int damage = (int)Math.Ceiling(rawDamage + ((this.n_lvl * 0.1) + 0.9));
                target.takePhysDamage(damage);
            }
            this.n_uses++;
            if (this.n_uses % 5 == 0)
            {
                this.levelUp();
            }
            return true;                        
        }
        else
            return false;                                
    </source-code>
