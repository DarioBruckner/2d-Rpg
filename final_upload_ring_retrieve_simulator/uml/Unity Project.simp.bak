        m_TextMesh = transform.Find("Text").GetComponent&lt;TextMeshPro&gt;();
    </source-code>
                  </source-codes>
                </operation>
                <operation id="o41abaf3a502349598c6ed02603932b7c98" cid="OP469" name="Setup" creation-date="07/02/2021 13:44:32" type="" type-id="" visibility="private">
                  <parameters>
                    <parameter id="p264ce44dd9c0475fb4052d141d7b3547" name="text" type="string" type-id="string" />
                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        m_TextMesh.SetText(text);
        m_TextMesh.ForceMeshUpdate();
        Vector2 textSize = m_TextMesh.GetRenderedValues(false);
        Vector2 padding = new Vector2(2f, 1f);
        Vector3 offset = new Vector3(-1f, -0.25f, 0);

        m_BackgroundSpriteRenderer.size = textSize + padding;
        m_BackgroundSpriteRenderer.transform.localPosition = new Vector3(m_BackgroundSpriteRenderer.size.x / 2, 0, 
                                                                m_BackgroundSpriteRenderer.transform.localPosition.z) + offset;
    </source-code>
                  </source-codes>
                </operation>
              </operations>
