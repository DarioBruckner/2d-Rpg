                              <numbering-level type="None" level="0" format="{0}." />
                            </numbering>
                          </style>
                          <p>
                            <t> Thunder erfolgreich ausgef�hrtError: nicht genug Magiekraft
</t>
                          </p>
                        </paragraph>
                      </content>
                    </text>
                  </documentation>
                  <source-codes>
                    <source-code language="CSharp">
        if (user.drainMP(8))
        {
            double rawDamage = (user.n_magicalMight / target.n_magicalResistance) + 1;
            System.Random rng = new System.Random();
            double crit = rng.Next(100) * ((user.n_agility / 100) + 1);
            if (crit &gt; 50)
                rawDamage *= 2;

            int damage = (int)Math.Ceiling(rawDamage + ((this.n_lvl * 0.1) + 0.9));
            target.takeMagicDamage(damage);
            this.n_uses++;
            if (this.n_uses % 5 == 0)
            {
                this.levelUp();
            }
            return true;                                  
        }
        else
            return false;                                
    </source-code>
