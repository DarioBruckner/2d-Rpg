                  </parameters>
                  <source-codes>
                    <source-code language="CSharp">
        switch (barnum) {
            case 1:
                healthBarChar1.SetHealth(health);
                break;
            case 2:
                healthBarChar2.SetHealth(health);
                break;
            case 3:
                healthBarChar3.SetHealth(health);
                break;
            case 4:
                healthBarChar4.SetHealth(health);
                break;
        }

        
    </source-code>
                  </source-codes>
                </operation>
